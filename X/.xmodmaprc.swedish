!
! Personal keyboard layout based on Swedish keyboard 
!
remove Lock = Caps_Lock
remove Control = Control_L
remove Control = Control_R
remove Mod5 = ISO_Level3_Shift
keysym Control_L = ISO_Level3_Shift
keysym Caps_Lock = Control_L
keysym Control_R = adiaeresis
keysym adiaeresis = Control_R
keysym ISO_Level3_Shift = aring
keysym Control_L = ISO_Level3_Shift
keysym aring = Alt_R
add Control = Control_L
add Control = Control_R
add Mod1 = Alt_R
add Mod5 = ISO_Level3_Shift
