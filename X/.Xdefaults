
!-------------------------------------------------------------------------------
 
URxvt.depth: 32
URxvt.geometry: 90x30
URxvt.transparent: false
URxvt.fading: 0
! URxvt.urgentOnBell: true
! URxvt.visualBell: true
URxvt.loginShell: true
URxvt.saveLines: 50
URxvt.internalBorder: 3
URxvt.lineSpace: 0
 
! Fonts
URxvt.allow_bold: false
!URxvt.font:xft:Monospace:size=11:style=regular:antialias=true, xft:WenQuanYi Micro Hei Mono:size=12:style=regular:antialias=true
URxvt.font:xft:Monospace:size=10.5:style=regular:antialias=true
URxvt.multichar_encoding: utf-8
URxvt.imLocale: zh_CN.UTF-8

! Fix font space
URxvt*letterSpace: -1

! Scrollbar
URxvt.scrollStyle: plain
URxvt.scrollBar: false
URxvt.scrollBar_right: true
URxvt.scrollBar_floating: true
URxvt.saveLines: 1000
 
! Perl extensions
!URxvt.perl-ext-common: default,matcher
!URxvt.matcher.button: 1
!URxvt.urlLauncher: /usr/bin/google-chrome-stable
 
! Cursor
URxvt.cursorBlink: false
URxvt.cursorColor: #657b83
URxvt.cursorUnderline: false
 
! Pointer
URxvt.pointerBlank: true

!URxvt.inheritPixmap: true

! {{{extension
URxvt*perl-ext-common: default,tabbed,matcher,clipboard,keyboard-select,url-select
! clipboard/(install xsel)
URxvt*keysym.M-w: perl:clipboard:copy
URxvt*keysym.C-y: perl:clipboard:paste
! keyboard-select
URxvt*keysym.M-Escape: perl:keyboard-select:activate
URxvt*keysym.C-s: perl:keyboard-select:search
! url-select
URxvt.keysym.M-u: perl:url-select:select_next
! }}}

 
